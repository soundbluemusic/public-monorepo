      },
      "en": {
        "word": "Company",
        "explanation": "An organization where people work to earn money. A corporate entity engaged in commercial activities.",
        "examples": [
          "회사에 가요. - I'm going to the company/work.",
          "어느 회사 다니세요? - Which company do you work for?",
          "\"How's work life?\" \"Good, my colleagues are all nice.\"",
          "Our company is an IT firm."
        ],
        "variations": {
          "formal": ["It is a company", "Corporation", "Enterprise"],
          "casual": ["Company~", "Workplace"],
          "short": ["Company", "Corp"]
        }
      }
    }
