{
  "name": "@soundblue/config",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": ["./dist/entry-client.js"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/soundbluemusic/public-monorepo",
    "directory": "packages/config"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": ["dist", "src/styles"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "default": "./dist/tailwind.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    },
    "./biome": {
      "types": "./dist/biome.d.ts",
      "default": "./dist/biome.js"
    },
    "./styles/tailwind.css": "./src/styles/tailwind.css",
    "./entry-client": {
      "types": "./dist/entry-client.d.ts",
      "default": "./dist/entry-client.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.12.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-router": {
      "optional": true
    }
  },
  "devDependencies": {
    "@inlang/paraglide-js": "^2.9.1",
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.12.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.9",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-build": "^7.4.0"
  }
}
